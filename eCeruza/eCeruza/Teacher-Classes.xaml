<Window x:Class="eCeruza.Teacher_Classes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:eCeruza"
        mc:Ignorable="d"
        Title="Teacher_Classes" Height="1024" Width="1440">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="lbl1" Content="Label" Grid.Column="0" Grid.Row="0" />
        <Label x:Name="lbl2" Content="Label" Grid.Column="1" Grid.Row="0" />
        <Label x:Name="lbl3" Content="Label" Grid.Column="2" Grid.Row="0" />
        <Label x:Name="lbl4" Content="Label" Grid.Column="0" Grid.Row="1" />
        <Label x:Name="lbl5" Content="Label" Grid.Column="1" Grid.Row="1" />
        <Label x:Name="lbl6" Content="Label" Grid.Column="2" Grid.Row="1" />
        <Label x:Name="lbl7" Content="Label" Grid.Column="0" Grid.Row="2" />
        <Label x:Name="lbl8" Content="Label" Grid.Column="1" Grid.Row="2" />
        <Label x:Name="lbl9" Content="Label" Grid.Column="2" Grid.Row="2" />
    </Grid>
</Window>
